/*рограммf для вычисления целочисленного логарифма по основанию 2.*/
#include <iostream>

using namespace std;

int FOL(int k){
    int z=0, n=1;
    while(n<=k){
        n *= 2;
        z++;
    }
    return z-1;
}

int main(){
    int t;
    float x;
    cin>>t;
    for(int i=0; i<t; i++){
        cin>>x;
        x = FOL(x);
        cout<<x<<endl;
    }
    return 0;
}
